# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'MatrixClientWithGA' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MatrixClientWithGA
  pod 'MatrixSDK', :git => 'https://github.com/aapierce0/matrix-ios-sdk.git', :commit => 'b143a5eb49bb0152fe3205216fed332b1496ba67'
  
  # The Google Analytics pod is not compatible with MatrixSDK
  pod 'Google/Analytics'
end
